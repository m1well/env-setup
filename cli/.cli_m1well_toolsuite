### cli m1well toolsuite ###
# export the user's home like
# 'export USER_HOME="/Users/<your-user-home>"; '
# export the m1well's toolsuite home like
# 'export M1WELL_TOOLSUITE_HOME="/Users/<your-user-home>/<path-to-toolsuite>"; '

alias m1well-update="bash ${M1WELL_TOOLSUITE_HOME}/env-setup/m1well-toolsuite.sh -u"
alias cheat="bash ${M1WELL_TOOLSUITE_HOME}/cheatsheet/cheatsheet.sh"
alias versions="bash ${M1WELL_TOOLSUITE_HOME}/versions/versions.sh"
alias randomizer="bash ${M1WELL_TOOLSUITE_HOME}/randomizer/randomizer.sh"
alias list="bash ${M1WELL_TOOLSUITE_HOME}/bashlist/bashlist.sh"

ln -sf ${M1WELL_TOOLSUITE_HOME}/env-setup/dotfiles/.cheatsheet ${USER_HOME}/.cheatsheet
