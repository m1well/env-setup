{
  "name": "m1well",
  "style": "Dark",
  "author": {
    "name": "m1well",
    "twitter-url": "https://www.twitter.com/m1well"
  },
  "colors": {
    "colorPage": "color-hex.com",
    "blue": "#1057E8 -> RGB(16,87,232)",
    "yellow": "E8A110",
    "green": "57E810",
    "red": "E81057",
    "black": "1E1E23"
  },
  "editor": {
    "backgroundColor": "#1E1E23",
    "altBackgroundColor": "#111111",
    "tintColor": "#9FBBF5",
    "tintColor2": "#E8E8E9",
    "textColor": "#D2D2D3",
    "toolbarBackgroundColor": "#343438",
    "toolbarIconColor": "#9FBBF5",
    "menuItemColor": "#9FBBF5",
    "shouldOverwriteFont": true
  },
  "styles": {
    "body": {
      "color": "#E8E8E9",
      "font": "AvenirNext-Regular",
      "size": 15
    },
    "title1": {
      "color": "#1057E8",
      "font": "AvenirNext-DemiBold",
      "size": 27
    },
    "title-mark1": {
      "color": "#E8A110",
      "font": "AvenirNext-DemiBold",
      "lineSpacing": 2,
      "paragraphSpacing": 18,
      "paragraphSpacingBefore": 13,
      "size": 27
    },
    "title2": {
      "color": "#1057E8",
      "font": "AvenirNext-DemiBold",
      "size": 22
    },
    "title-mark2": {
      "color": "#E8A110",
      "font": "AvenirNext-DemiBold",
      "lineSpacing": 1,
      "paragraphSpacing": 16,
      "paragraphSpacingBefore": 8,
      "size": 22
    },
    "title3": {
      "color": "#2767EA",
      "font": "AvenirNext-DemiBold",
      "size": 18
    },
    "title-mark3": {
      "color": "#EAAA27",
      "font": "AvenirNext-DemiBold",
      "lineSpacing": 1,
      "paragraphSpacing": 13,
      "paragraphSpacingBefore": 2,
      "size": 18
    },
    "title4": {
      "color": "#2767EA",
      "font": "AvenirNext-Regular",
      "size": 16,
      "type": "italic"
    },
    "title-mark4": {
      "color": "#EAAA27",
      "font": "AvenirNext-DemiBold",
      "lineSpacing": 1,
      "paragraphSpacing": 12,
      "paragraphSpacingBefore": 2,
      "size": 16,
      "type": "italic"
    },
    "bold": {
      "color": "#3F78EC",
      "font": "AvenirNext-DemiBold"
    },
    "bold-left-mark": {
      "color": "#ECB33F"
    },
    "bold-right-mark": {
      "color": "#ECB33F"
    },
    "boldItalic": {
      "color": "#5789EE",
      "font": "AvenirNext-DemiBoldItalic"
    },
    "boldItalic-left-mark": {
      "color": "#EEBD57"
    },
    "boldItalic-right-mark": {
      "color": "#EEBD57"
    },
    "italic": {
      "color": "#6F9AF1",
      "font": "AvenirNext-Italic"
    },
    "italic-left-mark": {
      "color": "#F1C66F"
    },
    "italic-right-mark": {
      "color": "#F1C66F"
    },
    "code": {
      "color": "#EAAA27",
      "font": "Courier New",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#2767EA"
    },
    "code-right-backtick": {
      "color": "#2767EA"
    },
    "quote-mark": {
      "color": "#B9800C",
      "font": "noteplanstate",
      "headIndent": 34,
      "size": 15
    },
    "quote-content": {
      "color": "#BBBBBD",
      "type": "italic"
    },
    "todo": {
      "color": "#E8A110",
      "font": "noteplanstate",
      "headIndent": 35,
      "size": 16
    },
    "done": {
      "color": "#C8C815"
    },
    "checked": {
      "color": "#3CA20B",
      "headIndent": 38
    },
    "checked-todo-characters": {
      "font": "noteplanstate",
      "headIndent": 35,
      "size": 15
    },
    "special-char": {
      "color": "#E8A110",
      "font": "Menlo-Regular",
      "size": 15
    },
    "link": {},
    "schedule-to-date-link": {},
    "done-date": {},
    "schedule-from-date-link": {},
    "note-title-link": {},
    "hashtag": {},
    "attag": {},
    "phonenumber": {},
    "code-fence": {
      "corner-radius": 6,
      "body": {
        "backgroundColor": "#343438"
      },
      "language-specifier": {
        "color": "#2767EA"
      },
      "fence-open": {
        "color": "#2767EA"
      },
      "fence-close": {
        "color": "#2767EA"
      }
    },
    "highlighted": {
      "backgroundColor": "#0B3CA2",
      "color": "#D2D2D3",
      "isRevealOnCursorRange": true,
      "matchPosition": 2,
      "regex": "(::)([^:]{1,})(::)"
    },
    "highlighted-left-colon": {
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 1,
      "regex": "(::)([^:]{1,})(::)"
    },
    "highlighted-right-colon": {
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 3,
      "regex": "(::)([^:]{1,})(::)"
    },
    "strikethrough": {
      "color": "#78787B",
      "isRevealOnCursorRange": true,
      "matchPosition": 2,
      "regex": "(~~)([^~]{1,})(~~)",
      "strikethroughStyle": 1
    },
    "strikethrough-left-tilde": {
      "color": "#78787B",
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 1,
      "regex": "(~~)([^~]{1,})(~~)"
    },
    "strikethrough-right-tilde": {
      "color": "#78787B",
      "isHiddenWithoutCursor": true,
      "isMarkdownCharacter": true,
      "isRevealOnCursorRange": true,
      "matchPosition": 3,
      "regex": "(~~)([^~]{1,})(~~)"
    },
    "todo-flagged-1-sign": {
      "backgroundColor": "#5C0622",
      "matchPosition": 2,
      "regex": "(^|\\v)(\\h*(\\*)\\h*(\\[ \\]\\h+|\\h+)\\!{1}\\h+.*)($|\\v)"
    },
    "todo-flagged-2-sign": {
      "backgroundColor": "#A20B3C",
      "matchPosition": 2,
      "regex": "(^|\\v)(\\h*(\\*)\\h*(\\[ \\]\\h+|\\h+)\\!{2}\\h+.*)($|\\v)"
    },
    "todo-checked-override": {
      "color": "#3CA20B",
      "matchPosition": 0,
      "regex": "(^\\h*[\\*]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )(.*)",
      "strikethroughStyle": 1
    },
    "todo-scheduled-override": {
      "color": "#F8E2B7",
      "matchPosition": 0,
      "regex": "(^\\h*[\\*]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "strikethroughStyle": 1
    },
    "todo-canceled-override": {
      "color": "#F8B7CC",
      "matchPosition": 0,
      "regex": "(^\\h*[\\*]{1} |^\\h*[0-9]+[\\.\\)] )(\\[-\\] )(.*)",
      "strikethroughStyle": 1
    },
    "todo-wife": {
      "backgroundColor": "#453004",
      "matchPosition": 2,
      "regex": "(^|\\v)(\\h*(\\*|-)\\h*(\\[ \\]\\h+|\\h+)\\Frau(:?)\\h+.*)($|\\v)"
    },
    "template-tags": {
      "color": "#8B6009",
      "font": "Menlo-Regular",
      "matchPosition": 0,
      "isHiddenWithoutCursor": false,
      "isRevealOnCursorRange": true,
      "regex": "(?>\\{\\{)[^)]{1,}((?>\\(\\)\\}\\})|(?>\\)\\}\\}))"
    },
    "timeblocks": {
      "color": "#F1C66F",
      "matchPosition": 0,
      "regex": "(?<!@done\\(\\d{4}-[01]\\d{1}-\\d{2})(^|\\s|T)((at|from)\\s*)?(([0-2]?\\d{1}|noon|midnight)(:[0-5]\\d)?|(?:[0-2]?\\d{1}|noon|midnight))(A\\.M\\.|P\\.M\\.|AM?|PM?|am?|pm?)?(\\s*(\\-|\\–|\\~|\\〜|to|\\?)\\s*([0-2]?\\d)(:[0-5]\\d)?(A\\.M\\.|P\\.M\\.|AM?|PM?|am?|pm?)?|$)",
      "type": "italic"
    }
  }
}
